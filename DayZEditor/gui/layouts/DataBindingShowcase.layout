FrameWidgetClass WindowFrame {
 visible 1
 size 800 600
 halign center_ref
 valign center_ref
 hexactpos 1
 vexactpos 1
 hexactsize 1
 vexactsize 1
 draggable 0
 scriptclass ""
 {
  WrapSpacerWidgetClass WindowContent {
   ignorepointer 1
   position 0 0
   size 1 1
   halign center_ref
   valign center_ref
   hexactpos 1
   vexactpos 1
   hexactsize 0
   vexactsize 0
   scriptclass "DataBindingShowcaseController"
   "no focus" 0
   Padding 5
   Margin 5
   "Size To Content H" 1
   "Size To Content V" 1
   content_halign center
   content_valign center
   {
    EditBoxWidgetClass WindowText {
     visible 1
     ignorepointer 0
     position 0 0
     size 0.8 50
     halign center_ref
     hexactpos 1
     vexactpos 1
     hexactsize 0
     vexactsize 1
     draggable 0
     scriptclass "ViewBinding"
     {
      ScriptParamsClass {
       Two_Way_Binding 1
      }
     }
    }
    ButtonWidgetClass WindowTextToggleValue {
     visible 1
     size 50 50
     halign right_ref
     valign center_ref
     hexactpos 1
     vexactpos 1
     hexactsize 1
     vexactsize 1
     scriptclass "ViewBinding"
     style Editor
     switch normal
     {
      ScriptParamsClass {
       Binding_Name "EvaluateEquation"
       Two_Way_Binding 1
      }
     }
    }
    SliderWidgetClass WindowSlider {
     visible 1
     position 0 0
     size 0.8 50
     halign center_ref
     hexactpos 1
     vexactpos 1
     hexactsize 0
     vexactsize 1
     scriptclass "ViewBinding"
     step 0.05
     {
      TextWidgetClass WindowSliderReadout {
       visible 1
       ignorepointer 1
       color 0 0 0 1
       size 150 50
       halign center_ref
       valign center_ref
       hexactpos 1
       vexactpos 1
       hexactsize 1
       vexactsize 1
       scriptclass "ViewBinding"
       font "gui/fonts/metron48"
       "exact text" 1
       "text halign" center
       "text valign" center
       {
        ScriptParamsClass {
         Binding_Name "WindowSlider"
        }
       }
      }
     }
     {
      ScriptParamsClass {
       Two_Way_Binding 1
      }
     }
    }
    CheckBoxWidgetClass WindowCheckBox {
     visible 1
     clipchildren 1
     size 50 50
     halign center_ref
     valign center_ref
     hexactpos 1
     vexactpos 1
     hexactsize 1
     vexactsize 1
     scaled 1
     scriptclass "ViewBinding"
     style Default
     {
      ScriptParamsClass {
       Two_Way_Binding 1
      }
     }
    }
    WrapSpacerWidgetClass WindowListWidgets {
     ignorepointer 1
     position 0 0
     size 1 250
     hexactpos 1
     vexactpos 1
     hexactsize 0
     vexactsize 1
     Padding 0
     Margin 0
     content_halign center
     content_valign center
    }
   }
   {
    ScriptParamsClass {
    }
   }
  }
 }
}